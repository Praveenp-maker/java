import java.util.*;
class Main {
  public static void main(String[] args) {
   int a[]={1,2,3,4};
    
   int b[]={2,3,4,5};
   int alen=a.length;
    int blen=b.length;
    int result[]=new int[alen+blen];
    System.arraycopy(a,0,result,0,alen);
    System.arraycopy(b,0,result,alen,blen);
    System.out.println(Arrays.toString(result));
      }
  
}
